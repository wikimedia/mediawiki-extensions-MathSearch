		"ext.math-mathml.styles": {
			"styles": "ext.math-mathml.css",
			"targets": [
				"desktop",
				"mobile"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "MathSearch/resources"
	},
	"SpecialPages": {
		"MathSearch": "SpecialMathSearch",
		"MlpEval": "SpecialMlpEval",
		"FormulaInfo": {
			"class": "FormulaInfo",
			"services": [
				"Math.Config"
			]
		},
